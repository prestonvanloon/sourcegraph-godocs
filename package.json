{
  "name": "sourcegraph-godocs",
  "title": "GoDocs",
  "publisher": "prestonvanloon",
  "version": "1.0.0",
  "main": "dist/extension.js",
  "description": "Sourcegraph extension for viewing Godocs. WORK-IN-PROGRESS: There is virtually no documentation on how to use the sourcegraph extension API.",
  "scripts": {
    "sourcegraph:prepublish": "parcel build src/extension.ts",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "author": "Preston Van Loon",
  "license": "MIT",
  "dependencies": {
    "parcel-bundler": "^1.10.3",
    "sourcegraph": "^18.0.0",
    "typescript": "^3.1.3"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "actions": [
      {
        "id": "godocs.open",
        "command": "open",
        "commandArguments": [
          "https://godoc.org/${get(context, 'godoc.path')}"
        ],
        "category": "godocs",
        "title": "Godocs link",
        "iconURL": "https://raw.githubusercontent.com/golang-samples/gopher-vector/master/gopher.svg",
        "actionItem": {
          "label": "Godocs",
          "description": "View this package on godocs.org"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "action": "godocs.open",
          "when": "0 == 0"
        }
      ]
    }
  },
  "devDependencies": {
    "gts": "^0.8.0",
    "typescript": "~2.8.0"
  }
}
